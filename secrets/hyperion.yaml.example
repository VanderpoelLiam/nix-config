# Example secrets file for hyperion
# Copy this to secrets/hyperion.yaml and encrypt with sops

# Format for Caddy environment file: CLOUDFLARE_API_TOKEN=token_value
cloudflare_api_token: |
  CLOUDFLARE_API_TOKEN=your-cloudflare-api-token-here

# Plain password for Pi-hole
pihole_password: "your-pihole-admin-password"
